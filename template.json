{
  "package": {
    "eslintConfig": {},
    "dependencies": {
      "@testing-library/jest-dom": "^5.5.0",
      "@testing-library/react": "^10.0.2",
      "@testing-library/user-event": "^10.0.1",
      "cross-env": "^7.0.2",
      "enzyme": "^3.11.0",
      "enzyme-adapter-react-16": "^1.15.2",
      "eslint": "^6.8.0",
      "eslint-config-airbnb": "^18.1.0",
      "eslint-config-prettier": "^6.10.1",
      "eslint-plugin-compat": "^3.5.1",
      "eslint-plugin-import": "^2.20.2",
      "eslint-plugin-jsx-a11y": "^6.2.3",
      "eslint-plugin-prettier": "^3.1.3",
      "eslint-plugin-react": "^7.19.0",
      "eslint-plugin-react-hooks": "^3.0.0",
      "husky": "^4.2.5",
      "lint-staged": "^10.1.3",
      "node-sass": "^4.13.1",
      "prettier": "^2.0.4",
      "react-router-dom": "^5.1.2",
      "rimraf": "^3.0.2",
      "source-map-explorer": "^2.4.2",
      "stylelint": "^13.3.2",
      "stylelint-config-prettier": "^8.0.1",
      "stylelint-config-standard": "^20.0.0",
      "stylelint-scss": "^3.16.1"
    }
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --config .prettierrc.js --write **/*.{css,html,js,jsx,json,md,scss,yml}",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint -c .eslintrc.js **/*.{js,jsx}",
    "lint:stylelint": "stylelint --config .stylelintrc.js **/*.{css,scss}",
    "prebuild": "rimraf build && npm run format && npm run lint && npm run test:ci",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom",
    "test:coverage": "cross-env CI=true npm test -- --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache"
  }
}
