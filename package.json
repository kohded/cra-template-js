    "stylelint-scss": "^3.16.1"
  },
  "description": "The unofficial base template for Create React App.",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "template",
    "template.json"
  ],
  "homepage": "https://github.com/kohded/cra-template-js#readme",
  "keywords": [
    "react",
    "create-react-app",
    "template"
  ],
  "license": "MIT",
  "main": "template.json",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohded/cra-template-js.git"
  },
  "scripts": {
    "format": "prettier --config ./template/.prettierrc.js --write **/*.{css,html,js,jsx,json,md,scss,yml} --ignore-path=template/.prettierignore",
    "lint": "npm run format && npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint -c ./template/.eslintrc.js **/*.{js,jsx}",
    "lint:stylelint": "stylelint --config ./template/.stylelintrc.js **/*.{css,scss}"
  }
